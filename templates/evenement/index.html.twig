
{% extends 'base.html.twig' %}

{% block title %}Evenement index{% endblock %}

{% block body %}
    <h1>Evenement index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>NomClub</th>
                <th>FonctionClub</th>
                <th>DescripitonClub</th>
                <th>LogoClub</th>
                <th>TresorieClub</th>
                <th>LocalClub</th>
                <th>NombreStudentClub</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for evenement in evenements %}
            <tr>
                <td>{{ evenement.id }}</td>
                <td>{{ evenement.NomClub }}</td>
                <td>{{ evenement.FonctionClub }}</td>
                <td>{{ evenement.DescripitonClub }}</td>
                <td>{{ evenement.LogoClub }}</td>
                <td>{{ evenement.TresorieClub }}</td>
                <td>{{ evenement.LocalClub }}</td>
                <td>{{ evenement.NombreStudentClub }}</td>
                <td>
                    <a href="{{ path('app_evenement_show', {'id': evenement.id}) }}">show</a>
                    <a href="{{ path('app_evenement_edit', {'id': evenement.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_evenement_new') }}">Create new</a>
{% endblock %}
