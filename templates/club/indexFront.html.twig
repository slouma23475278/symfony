{% extends 'baseFront.html.twig' %}

{% block body %}
<section class="categories-section spad">
    <div class="container">
        <div class="section-title">
            <h2>Our Clubs </h2>
            <p>You can see Our club here and enroll if you want .</p>
            <br>
        </div>
        <div class="row">

            {% for club in clubs %}
            <!-- categorie -->
                            <div class="col-lg-4 col-md-6">
                                <div class="categorie-item">
                                    <div class="ci-thumb set-bg" data-setbg="{{ asset('front-assets/img/categories/7.jpg') }}"></div>
                                    <div class="ci-text">
                                        <h5>{{ club.NomClub }}</h5>
                                        <p>{{ club.DescriptionClub }}</p>
                                        <span>{{ club.NombreStudentClub }}</span>
                                    </div>
                                </div>
                            </div>
                            {% else %}
                            <br>
                                <h2>no records found</h2>
                            <br>
                        {% endfor %}
                    </div>
                </div>
            </section>

{% endblock %}